bigvm:
	CFLAGS = $(CFLAGS)
	make -f foot emul.exe

emul.exe : $(OFILES) 
	@ echo $** > linkopts
	@ echo  $(CFLAGS) > copts
	$(RM) vdate.c
	$(CC) @copts @linkopts $(LDFLAGS) /e$@
	del linkopts
	del copts
	@ echo "Executable is now named '$@'"
   
main.o  :       lispemul.h address.h lsptypes.h adr68k.h stack.h lspglob.h lispmap.h ifpage.h iopage.h return.h debug.h profile.h


													

