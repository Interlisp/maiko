	$(CC) @copts ../src/lisp2c.c -I ../inc -c $@
	del copts

rplcons.obj : ../src/rplcons.c
	@ echo  $(CFLAGS) > copts
	$(CC) @copts ../src/rplcons.c -I ../inc -c $@
	del copts

z2.obj : ../src/z2.c
	@ echo  $(CFLAGS) > copts
	$(CC) @copts ../src/z2.c -I ../inc -c $@
	del copts

