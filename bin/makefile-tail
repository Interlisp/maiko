################################################################################
# Miscellaneous targets
#	.c.s. should always have -O
################################################################################

clean cleanup:
	$(RM) -r $(OBJECTDIR) $(OSARCHDIR)

.c.o:
	$(CC) $(RFLAGS) $*.c -o $@

.c.s:
	$(CC) -S $(CFLAGS) -I$(INCDIR) $(SRCDIR)$*.c -o $(SRCDIR)$@

cxref:
	cxref -c $(DFLAGS) -I$(INCDIR) $(SRCDIR)*.c -o ../all.cxref
